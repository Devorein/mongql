{
  "name": "mongql",
  "version": "1.2.9",
  "description": "Create graphql Schema from mongoose Schema",
  "keywords": [
    "codegen",
    "schema-generator",
    "mongoose-to-graphql",
    "graphql-schema"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com:Devorein/mongql.git"
  },
  "license": "MIT",
  "author": "Devon Reid <devorein00@gmail.com>",
  "main": "dist/index",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run emit-declarations && npx tsc --removeComments",
    "build:watch": "npx tsc --incremental true -w",
    "clean": "npx rimraf ./dist",
    "emit-declarations": "npx tsc --declaration",
    "lint": "npx eslint --ext .ts,.js src/ --fix --color",
    "prepack": "npm run test_build",
    "prepublishOnly": "npm run test_build",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "npx jest --runInBand ./tests/integration",
    "test:unit": "npx jest --runInBand ./tests/unit",
    "test_build": "npm run test && npm run build",
    "transpile": "npx babel src --out-dir dist --extensions .ts",
    "typecheck": "npx tsc --noEmit --incremental false",
    "preversion": "npm run test"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.10.5",
    "@graphql-tools/resolvers-composition": "^6.0.14",
    "@graphql-tools/schema": "^6.0.14",
    "colors": "^1.4.0",
    "fs-extra": "^9.0.1",
    "graphql": "^15.3.0",
    "graphql-extra": "^0.2.2",
    "graphql-scalars": "^1.2.6",
    "graphql-tag": "^2.10.4",
    "mkdirp": "^1.0.4",
    "mongoose": "^5.9.27",
    "pluralize": "^8.0.0",
    "voca": "^1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/eslint": "^7.2.0",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.7",
    "@types/mongoose": "^5.7.34",
    "@types/node": "^14.0.27",
    "@types/pluralize": "0.0.29",
    "@types/voca": "^1.4.0",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "^7.6.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "sort-package-json": "^1.44.0",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.7"
  }
}