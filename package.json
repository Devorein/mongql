{
  "name": "mongql",
  "version": "1.2.9",
  "description": "Create graphql Schema from mongoose Schema",
  "keywords": [
    "codegen",
    "schema-generator",
    "mongoose-to-graphql",
    "graphql-schema"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com:Devorein/mongql.git"
  },
  "license": "MIT",
  "author": "Devon Reid <devorein00@gmail.com>",
  "main": "dist/src/Mongql.js",
  "scripts": {
    "build": "npx rimraf ./dist && npx babel src --out-dir dist/src && npx babel utils --out-dir dist/utils",
    "changelog": "npx conventional-changelog -i ./CHANGELOG.md -s -r 0 -u false && git add CHANGELOG.md",
    "lint": "npx eslint --fix",
    "prepack": "npm run test_build",
    "prepublishOnly": "npm run test_build",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "npx jest --runInBand ./tests/integration",
    "test:unit": "npx jest --runInBand ./tests/unit",
    "test_build": "npm run test && npm run build",
    "preversion": "npm run test && npm run changelog"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint"
    ],
    "*.json": [
      "npx sort-package-json"
    ]
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.10.5",
    "@graphql-tools/load-files": "^6.0.14",
    "@graphql-tools/merge": "^6.0.14",
    "@graphql-tools/resolvers-composition": "^6.0.14",
    "@graphql-tools/schema": "^6.0.14",
    "colors": "^1.4.0",
    "fs-extra": "^9.0.1",
    "graphql": "^15.3.0",
    "graphql-extra": "^0.2.2",
    "graphql-scalars": "^1.2.2",
    "graphql-tag": "^2.10.4",
    "mkdirp": "^1.0.4",
    "mongoose": "^5.9.25",
    "pluralize": "^8.0.0",
    "voca": "^1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@size-limit/preset-small-lib": "^4.5.5",
    "babel-eslint": "^10.1.0",
    "babel-preset-minify": "^0.5.1",
    "conventional-changelog-cli": "^2.0.34",
    "eslint": "^7.4.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "jest-html-reporter": "^3.1.3",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "sort-package-json": "^1.44.0",
    "typescript": "^3.9.7"
  }
}
